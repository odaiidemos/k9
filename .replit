modules = ["python-3.11", "web", "nodejs-20"]
run = "run"

[nix]
channel = "stable-25_05"
packages = ["freetype", "glibcLocales", "libxcrypt", "openssl", "postgresql", "rsync", "sqlite"]

[deployment]
deploymentTarget = "autoscale"
run = ["gunicorn", "--bind", "0.0.0.0:5000", "main:app"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "FastAPI Backend"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "React Frontend"

[[workflows.workflow]]
name = "FastAPI Backend"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd backend_fastapi && uvicorn app.main:app --host 0.0.0.0 --port 8000 --reload"
waitForPort = 8000

[[workflows.workflow]]
name = "React Frontend"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd k9-frontend && npm run dev"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8000
externalPort = 8000

